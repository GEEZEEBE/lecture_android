<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swe.ohsanghun.androidappwithoutevening_activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <!-- <uses-permission android:name="android.permission.READ_PHONE_STATE" /> -->
    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.ohsanghun.androidappwithoutevening.ListViewActivity" >
        </activity>
        <activity
            android:name="com.example.ohsanghun.androidappwithoutevening.ExplicitIntentResponseActivity"
            android:label="@string/title_activity_sub"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.ohsanghun.androidappwithoutevening.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ohsanghun.androidappwithoutevening.ExplicitIntentSimpleActivity"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar" />
        <activity android:name="com.example.ohsanghun.androidappwithoutevening.ExplicitIntentRequestActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.swe.ohsanghun.androidappwithoutevening_activity.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>