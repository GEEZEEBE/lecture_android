<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ohsanghun.awe_widget"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.ohsanghun.awe_widget.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ohsanghun.awe_widget.ButtonActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.TextViewActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.CheckBoxActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.SpinnerActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.ProgressBarActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.SeekBarActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.EditTextActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.QuickContactBadgeActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.RadioButtonActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.RatingBarActivity" />
        <activity android:name="com.example.ohsanghun.awe_widget.SwitchActivity" />
    </application>

</manifest>