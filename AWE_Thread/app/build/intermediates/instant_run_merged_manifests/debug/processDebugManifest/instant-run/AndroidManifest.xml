<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ohsanghun.awe_thread"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.ohsanghun.awe_thread.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ohsanghun.awe_thread.ErrorThreadActivity" />
        <activity android:name="com.example.ohsanghun.awe_thread.HandlerThreadActivity" />
        <activity android:name="com.example.ohsanghun.awe_thread.HandlerThreadWithMessageActivity" />
        <activity android:name="com.example.ohsanghun.awe_thread.WatchTimerActivity" />
        <activity android:name="com.example.ohsanghun.awe_thread.StopwatchActivity" />
        <activity android:name="com.example.ohsanghun.awe_thread.ViewPostActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.ohsanghun.awe_thread.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>